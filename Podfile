use_frameworks!

source 'https://github.com/CocoaPods/Specs.git'
platform :ios, '8.0'

#Podfile.lock should always be checked in. It's the record of what's been built in the past (and released to the store). You will be very sad if you do not have those.

target 'LotsaBGDispatch' do

#This version of ReSwift was behaving well and not triggering the Swift 3 updater. Probably not
#   necessary in the future (Nov 7 2016)
 pod 'ReSwift', :git => 'https://github.com/ReSwift/ReSwift.git', :commit =>  'a560b66dae96a167e2b79c9c11aa0a3ad93c2df6'
end
